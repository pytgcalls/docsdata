        app = NTgCalls()

        await app.change_stream(
            -1001185324811,
            MediaDescription(
                AudioDescription(
                    input_mode=InputMode.File,
                    input="foxes.pcm",
                    sample_rate=96000,
                    bits_per_sample=16,
                    channel_count=2,
                ),
            )
        )
